<table>
  <tr>
    <th>Name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @ones.each do |one| %>
  <tr>
    <td><%= one.name %></td>
    <td><%= link_to 'Show', one %></td>
    <td><%= link_to 'Edit', edit_one_path(one) %></td>
